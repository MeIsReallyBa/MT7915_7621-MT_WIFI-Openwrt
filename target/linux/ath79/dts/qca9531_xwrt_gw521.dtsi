/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca953x.dtsi"

/ {
	keys {
		compatible = "gpio-keys";

		pinctrl-names = "default";

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&jtag_disable_pins &led_lte_green_pin &led_wifi_pin &led_lte_red_pin>;

		eth {
			label = "green:eth";
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
		};

		lte_yellow {
			label = "lte:yellow";
			gpios = <&gpio 2 GPIO_ACTIVE_LOW>;
		};

		lte_green {
			label = "lte:green";
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
		};

		wifi {
			label = "green:wifi";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};

		lte_red {
			label = "lte:red";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};
	};
};

&pinmux {
	led_lte_green_pin: pinmux_lte_green_pin {
		pinctrl-single,bits = <0x8 0x0 0xff000000>;
	};

	led_wifi_pin: pinmux_wifi_pin {
		pinctrl-single,bits = <0xc 0x0 0x00ff0000>;
	};

	led_lte_red_pin: pinmux_lte_red_pin {
		pinctrl-single,bits = <0x10 0x0 0x000000ff>;
	};
};

&pcie0 {
	status = "okay";

	wifi@0,0 {
		compatible = "qcom,ath10k";
		reg = <0 0 0 0 0>;
		device_type = "pci";
	};
};

&uart {
	status = "okay";
};

&usb0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&spi {
	status = "okay";

	num-cs = <2>;
	cs-gpios = <0>, <0>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x040000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x040000 0x010000>;
				read-only;
			};

			nor_firmware: partition@50000 {
				label = "nor_firmware";
				reg = <0x050000 0xfa0000>;
			};

			art: partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x010000>;
			};
		};
	};

	nand_flash: flash@1 {
		compatible = "spi-nand";
		reg = <1>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			nand_kernel: partition@0 {
				label = "nand_kernel";
				reg = <0x000000 0x0400000>;
			};

			nand_ubi: partition@200000 {
				label = "nand_ubi";
				reg = <0x400000 0x7c00000>;
			};
		};
	};
};

&eth0 {
	status = "disabled";
};

/* eth1 is 'eth0' on system */
&eth1 {
	mtd-mac-address = <&art 0x0>;
	nvmem-cells = <&macaddr_art_0>;
	nvmem-cell-names = "mac-address";
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x1000>;
};

&art {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_art_0: macaddr@0 {
		reg = <0x0 0x6>;
	};
};
